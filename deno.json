{
  "name": "@silverbulletmd/silverbullet",
  "version": "2.0.0-pre4",
  "exports": {
    "./syscall": "./plug-api/syscall.ts",
    "./syscalls": "./plug-api/syscalls.ts",
    "./lib/json": "./plug-api/lib/json.ts",
    "./lib/tree": "./plug-api/lib/tree.ts",
    "./lib/attribute": "./plug-api/lib/attribute.ts",
    "./lib/page_ref": "./plug-api/lib/page_ref.ts",
    "./lib/resolve": "./plug-api/lib/resolve.ts",
    "./lib/frontmatter": "./plug-api/lib/frontmatter.ts",
    "./type/client": "./type/client.ts",
    "./type/command": "./type/command.ts",
    "./type/config": "./type/config.ts",
    "./type/datastore": "./type/datastore.ts",
    "./type/event": "./type/event.ts",
    "./type/index": "./type/index.ts",
    "./type/rpc": "./type/rpc.ts",
    "./lib/tags": "./plug-api/lib/tags.ts"
  },
  "publish": {
    "exclude": [
      "website",
      "CHANGELOG.md",
      "**/*.md"
    ]
  },
  "tasks": {
    "clean": "rm -rf dist dist_client_bundle dist_plug_bundle",
    "deep-clean-mac": "rm -f deno.lock && rm -rf $HOME/Library/Caches/deno && deno task clean",
    "install": "deno install -g -f -A --import-map deno.json silverbullet.ts",
    "check": "find . -name '*.ts*' -not -path \"*/node_modules/*\" | xargs deno check",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "test": "deno test -A",
    "bench": "deno bench",
    "build": "rm -rf dist dist_client_bundle dist_plug_bundle && deno run -A build_plugs.ts && deno run -A build_web.ts --production && deno task compile",
    "plugs": "deno run -A build_plugs.ts",
    "server": "deno run -A  --check silverbullet.ts",
    "generate": "./scripts/generate.sh",
    "docker": "./scripts/build_docker.sh",
    "watch-web": "deno run -A --check build_web.ts --watch",
    "watch-server": "deno run -A --check --watch silverbullet.ts",
    "watch-plugs": "deno run -A --check build_plugs.ts -w",
    "watch-all": "deno task watch-web & deno task watch-plugs & deno task watch-server",
    "bundle": "deno run -A build_bundle.ts",
    "compile": "deno task bundle && deno compile -A -o silverbullet dist/silverbullet.js",
    "server:dist:linux-x86_64": "deno task bundle && deno compile -A --target x86_64-unknown-linux-gnu -o silverbullet dist/silverbullet.js",
    "server:zip:linux-x86_64": "deno task server:dist:linux-x86_64 && zip silverbullet-server-linux-x86_64.zip silverbullet",
    "server:dist:linux-aarch64": "deno task bundle && deno compile -A --target aarch64-unknown-linux-gnu -o silverbullet dist/silverbullet.js",
    "server:zip:linux-aarch64": "deno task server:dist:linux-aarch64 && zip silverbullet-server-linux-aarch64.zip silverbullet",
    "server:dist:darwin-x86_64": "deno task bundle && deno compile -A --target x86_64-apple-darwin -o silverbullet dist/silverbullet.js",
    "server:zip:darwin-x86_64": "deno task server:dist:darwin-x86_64 && zip silverbullet-server-darwin-x86_64.zip silverbullet",
    "server:dist:darwin-aarch64": "deno task bundle && deno compile -A --target aarch64-apple-darwin -o silverbullet dist/silverbullet.js",
    "server:zip:darwin-aarch64": "deno task server:dist:darwin-aarch64 && zip silverbullet-server-darwin-aarch64.zip silverbullet",
    "server:dist:windows-x86_64": "deno task bundle && deno compile -A  --target x86_64-pc-windows-msvc -o silverbullet.exe dist/silverbullet.js",
    "server:zip:windows-x86_64": "deno task server:dist:windows-x86_64 && zip silverbullet-server-windows-x86_64.zip silverbullet.exe"
  },
  "lint": {
    "exclude": [
      "dist",
      "dist_client_bundle",
      "dist_plug_bundle",
      "cmd/test_space",
      "cmd/test/test_space"
    ],
    "rules": {
      "exclude": [
        "no-explicit-any",
        "no-slow-types",
        "jsx-boolean-value"
      ]
    }
  },
  "fmt": {
    "exclude": [
      "dist*",
      "website",
      "test_space",
      "**/*.md",
      "**/*.html",
      "**/*.js",
      "**/*.json"
    ]
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact@10.26.4"
  },
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "@silverbulletmd/silverbullet/syscall": "./plug-api/syscall.ts",
    "@silverbulletmd/silverbullet/syscalls": "./plug-api/syscalls.ts",
    "@silverbulletmd/silverbullet/type/client": "./type/client.ts",
    "@silverbulletmd/silverbullet/type/command": "./type/command.ts",
    "@silverbulletmd/silverbullet/type/config": "./type/config.ts",
    "@silverbulletmd/silverbullet/type/datastore": "./type/datastore.ts",
    "@silverbulletmd/silverbullet/type/event": "./type/event.ts",
    "@silverbulletmd/silverbullet/type/index": "./type/index.ts",
    "@silverbulletmd/silverbullet/type/rpc": "./type/rpc.ts",
    "@silverbulletmd/silverbullet/lib/json": "./plug-api/lib/json.ts",
    "@silverbulletmd/silverbullet/lib/tree": "./plug-api/lib/tree.ts",
    "@silverbulletmd/silverbullet/lib/attribute": "./plug-api/lib/attribute.ts",
    "@silverbulletmd/silverbullet/lib/page_ref": "./plug-api/lib/page_ref.ts",
    "@silverbulletmd/silverbullet/lib/resolve": "./plug-api/lib/resolve.ts",
    "@silverbulletmd/silverbullet/lib/query_expression": "./plug-api/lib/query_expression.ts",
    "@silverbulletmd/silverbullet/lib/native_fetch": "./plug-api/lib/native_fetch.ts",
    "@silverbulletmd/silverbullet/lib/frontmatter": "./plug-api/lib/frontmatter.ts",
    "@silverbulletmd/silverbullet/lib/tags": "./plug-api/lib/tags.ts",
    "@codemirror/autocomplete": "npm:@codemirror/autocomplete@6.18.6",
    "@codemirror/commands": "npm:@codemirror/commands@6.8.1",
    "@codemirror/lang-css": "npm:@codemirror/lang-css@6.3.1",
    "@codemirror/lang-html": "npm:@codemirror/lang-html@6.4.9",
    "@codemirror/lang-javascript": "npm:@codemirror/lang-javascript@6.2.4",
    "@codemirror/lang-markdown": "npm:@codemirror/lang-markdown@6.3.3",
    "@codemirror/language": "npm:@codemirror/language@6.11.1",
    "@codemirror/legacy-modes": "npm:@codemirror/legacy-modes@6.5.1",
    "@codemirror/legacy-modes/mode/yaml": "npm:@codemirror/legacy-modes@6.5.1/mode/yaml",
    "@codemirror/lint": "npm:@codemirror/lint@6.8.5",
    "@codemirror/search": "npm:@codemirror/search@6.5.11",
    "@codemirror/state": "npm:@codemirror/state@6.5.2",
    "@codemirror/view": "npm:@codemirror/view@6.37.2",
    "@lezer/common": "npm:@lezer/common@1.2.3",
    "@lezer/css": "npm:@lezer/css@1.2.1",
    "@lezer/highlight": "npm:@lezer/highlight@1.2.1",
    "@lezer/html": "npm:@lezer/html@1.3.10",
    "@lezer/javascript": "npm:@lezer/javascript@1.5.1",
    "@lezer/lr": "npm:@lezer/lr@1.4.2",
    "@lezer/markdown": "npm:@lezer/markdown@1.4.3",
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.1",
    "@replit/codemirror-lang-nix": "npm:@replit/codemirror-lang-nix@6.0.1",
    "@replit/codemirror-vim": "npm:@replit/codemirror-vim@6.3.0",
    "@std/assert": "jsr:@std/assert@^1.0.1",
    "@std/cli": "jsr:@std/cli@^1.0.1",
    "@std/fs": "jsr:@std/fs@^1.0.18",
    "@std/io": "jsr:@std/io@^0.225.2",
    "@std/io/read-all": "jsr:@std/io@^0.225.2/read-all",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "@std/yaml": "jsr:@std/yaml@^1.0.8",
    "crelt": "npm:crelt@1.0.6",
    "denosass": "https://deno.land/x/denosass@1.0.6/mod.ts",
    "djwt": "https://deno.land/x/djwt@v3.0.2/mod.ts",
    "esbuild": "https://deno.land/x/esbuild@v0.25.1/mod.js",
    "fake-indexeddb/auto": "npm:fake-indexeddb@6.0.1/auto",
    "fast-diff": "npm:fast-diff@1.3.0",
    "fuse": "https://deno.land/x/fuse@v6.4.1/dist/fuse.esm.min.js",
    "gitignore-parser": "npm:gitignore-parser@0.0.2",
    "idb": "npm:idb@8.0.3",
    "hono": "jsr:@hono/hono@4.8.0",
    "hono/cookie": "jsr:@hono/hono@4.8.0/cookie",
    "hono/validator": "jsr:@hono/hono@4.8.0/validator",
    "js-yaml": "npm:js-yaml@4.1.0",
    "mimetypes": "https://deno.land/x/mimetypes@v1.0.0/mod.ts",
    "porter-stemmer": "npm:porter-stemmer@0.9.1",
    "preact": "npm:preact@10.26.9",
    "preact-feather": "npm:preact-feather@4.2.1",
    "preact-feather/types": "npm:preact-feather@4.2.1/dist/types",
    "react-icons/md": "https://deno.land/x/react_icons_md@1.0.9/mod.ts",
    "react-icons/md/": "https://deno.land/x/react_icons_md@1.0.9/ico/",
    "react-icons/types": "https://deno.land/x/react_icons@1.0.9/mod.ts",
    "s3_lite_client/": "https://deno.land/x/s3_lite_client@0.4.0/",
    "style-mod": "npm:style-mod@4.1.2",
    "turndown": "npm:turndown@7.2.0",
    "ajv": "npm:ajv@8.17.1",
    "turndown-plugin-gfm": "npm:@joplin/turndown-plugin-gfm@1.0.62"
  }
}
